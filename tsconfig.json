{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@/styles": ["src/styles/*"],
			"@/components": ["src/components/*"],
			"@/models": ["src/models/*"],
			"@/pages": ["src/pages/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
